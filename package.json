    "ios": "yarn workspace native ios",
    "web": "yarn workspace web web",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "android": "expo run:android",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
