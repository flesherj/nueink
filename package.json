{
  "name": "nueink",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:flesherj/nueink.git",
  "author": "James Flesher <james@luminatehealth.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "transactions:clear": "yarn workspace @nueink/aws clear-transactions",
    "build": "yarn build:core && yarn build:aws && yarn build:ui && yarn build:plaid && yarn build:sdk && yarn build:ynab",
    "build:aws": "yarn workspace @nueink/aws build",
    "build:core": "yarn workspace @nueink/core build",
    "build:plaid": "yarn workspace @nueink/plaid build",
    "build:sdk": "yarn workspace @nueink/sdk build",
    "build:ui": "yarn workspace @nueink/ui build",
    "build:ynab": "yarn workspace @nueink/ynab build",
    "typecheck": "yarn typecheck:core && yarn typecheck:aws && yarn typecheck:ui && yarn typecheck:plaid && yarn typecheck:ynab && yarn typecheck:sdk",
    "typecheck:aws": "yarn workspace @nueink/aws typecheck",
    "typecheck:core": "yarn workspace @nueink/core typecheck",
    "typecheck:plaid": "yarn workspace @nueink/plaid typecheck",
    "typecheck:ui": "yarn workspace @nueink/ui typecheck",
    "typecheck:ynab": "yarn workspace @nueink/ynab typecheck",
    "typecheck:sdk": "yarn workspace @nueink/sdk typecheck",
    "presandbox:dev": "find packages/aws/amplify -name '*.js' -type f -delete && rm -f packages/aws/.amplify/tsconfig.tsbuildinfo",
    "sandbox:clean": "rm -rf packages/aws/.amplify/artifacts",
    "sandbox:dev": "yarn sandbox:clean && yarn workspace @nueink/aws ampx sandbox --profile solotech --identifier dev",
    "sandbox:dev:delete": "yarn workspace @nueink/aws ampx sandbox delete --profile solotech --identifier dev",
    "sandbox:watch": "cd packages/aws && ./watch-packages.sh",
    "sandbox:redeploy:api": "yarn workspace @nueink/aws touch:api",
    "sandbox:redeploy:financial:connect": "yarn workspace @nueink/aws touch:financial:connect",
    "sandbox:redeploy:financial:sync": "yarn workspace @nueink/aws touch:financial:sync",
    "ios": "./apps/build.sh native ios",
    "ios:dev": "./apps/build.sh native ios dev",
    "ios:clean": "rm -Rf $(xcrun simctl get_app_container booted com.nueink.app data)",
    "android": "./apps/build.sh native android",
    "android:dev": "./apps/build.sh native android dev",
    "native:start": "./apps/build.sh native start",
    "web": "./apps/build.sh web start",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.840.0",
    "@types/amplify": "^1.1.28",
    "@types/aws-lambda": "^8.10.150",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "aws-cdk-lib": "^2.189.1",
    "constructs": "^10.4.2",
    "cors": "^2.8.5",
    "esbuild": "^0.25.5",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "prettier": "3.6.1",
    "serverless-http": "^3.2.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-amplify/react-native": "^1.1.10",
    "@aws-amplify/rtn-web-browser": "^1.1.4",
    "@aws-amplify/ui-react-native": "^2.5.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "aws-amplify": "^6.15.1",
    "deepmerge": "^4.3.1",
    "expo": "~53.0.16",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-get-random-values": "^1.11.0",
    "react-native-safe-area-context": "^5.5.0",
    "react-native-url-polyfill": "^2.0.0",
    "uuid": "^11.1.0"
  }
}
